{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "concurrently \"expo start\" \"yarn watch-templates\"",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "build-templates": "babel --config-file ./babel-templates.config.js templates/*.jsx -d dist/templates --out-file-extension .template",
    "watch-templates": "nodemon --watch templates --ext jsx --exec yarn build-templates"
  },
  "dependencies": {
    "@klutchcard/alloy-js": "1.4.33",
    "@klutchcard/klutch-components": "0.1.84",
    "@expo-google-fonts/inter": "^0.1.0",
    "@expo/metro-config": "^0.1.61",
    "expo": "^42.0.0",
    "expo-app-loading": "^1.0.1",
    "expo-asset": "~8.3.3",
    "expo-file-system": "~11.1.3",
    "expo-haptics": "~10.1.0",
    "expo-status-bar": "~1.0.4",
    "moment": "^2.29.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-42.0.0.tar.gz",
    "react-native-svg": "12.1.1",
    "react-native-web": "~0.13.12",
    "react-router-native": "^5.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "~7.9.0",
    "@babel/preset-react": "^7.13.13",
    "@babel/runtime": "^7.13.10",
    "@types/react": "~16.9.35",
    "@types/react-native": "~0.63.2",
    "concurrently": "^6.0.2",
    "nodemon": "^2.0.7",
    "typescript": "~4.0.0"
  },
  "private": true
}
